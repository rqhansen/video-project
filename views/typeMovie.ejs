    <link rel="stylesheet" href="/public/stylesheets/reset.css">
    <link rel="stylesheet" href="/public/stylesheets/common.css">
    <link rel="stylesheet" href="/public/stylesheets/header.css">
    <link rel="stylesheet" href="/public/stylesheets/componentsheets/titleBar.css">
    <link rel="stylesheet" href="/public/stylesheets/typeMovie/index.css">
    <link rel="stylesheet" href="/public/stylesheets/footer.css">
    <script src="/public/javascripts/baiduAnalysis.js"></script>
